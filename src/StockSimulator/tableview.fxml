<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane prefHeight="400.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="StockSimulator.Controllers.TableViewController">

   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <Label style="-fx-font: NORMAL 20 Tahoma;" text="Stock Exchange" GridPane.columnIndex="0" GridPane.rowIndex="0">
   </Label>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
   <HBox alignment="TOP_CENTER" spacing="10" GridPane.rowIndex="1">
      <Button fx:id="ibmButton" onAction="#showIbmTableView" text="IBM" />
      <Button fx:id="googButton" onAction="#showGoogTableView" text="Goog" />
   </HBox>
   <Pane fx:id="tablePane" GridPane.rowIndex="2" />
</GridPane>
